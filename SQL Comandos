DROP DATABASE los_mosquitos

CREATE DATABASE los_mosquitos CHARACTER SET = utf8;

CREATE TABLE Temperature (
	idTemp INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
	Temp FLOAT(4,2),
	Fecha DATE,
	Hora TIME
);

CREATE TABLE Humidity (
	idHum INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
	Hum FLOAT(4,2),
	Fecha DATE,
	Hora TIME
);

CREATE TABLE Light (
	idLight INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
	Li FLOAT(4,2),
	Fecha DATE,
	Hora TIME
);





